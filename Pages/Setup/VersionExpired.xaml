<Page x:Class="Berichtsheft.Pages.Setup.VersionExpiredä"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Berichtsheft.Pages.Setup"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800" Foreground="White" FontFamily="{StaticResource EncodeSans}" FontSize="16"
      Title="VersionExpiredä">

        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Oh oh!" HorizontalAlignment="Center" Margin="0,0,0,10" FontSize="32" FontFamily="{StaticResource EncodeSansCondensedBold}" />
            <TextBlock Text="Dieser Alpha-Release ist leider veraltet. Bitte besorge dir einere neue Version" HorizontalAlignment="Center" Margin="0,0,0,5" />
            <TextBlock x:Name="txt_actualVersion" Text="Aktuelle Version: " FontSize="12" HorizontalAlignment="Center" />
        </StackPanel>
</Page>
